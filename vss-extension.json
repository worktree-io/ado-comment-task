{
  "manifestVersion": 1,
  "id": "worktree-comment",
  "name": "Worktree",
  "version": "1.0.5",
  "publisher": "worktree-io",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Post an Open workspace link on every new work item",
  "galleryFlags": ["Public"],
  "categories": ["Azure Pipelines"],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "WorktreeCommentTask"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "WorktreeComment",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "WorktreeCommentTask"
      }
    }
  ]
}
